<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Styles/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Styles/contentstyle.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Content/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
     <script src="@Url.Content("~/Content/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Scripts/jquery.cookie.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Scripts/jquery.hotkeys.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/Scripts/jquery.jstree.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
            <div id="TopBox">
                <img id="imgBIMXchangeLogo" src="@Url.Content("~/Content/Images/BIMXchange-logo.png")">
            </div>
            <div id="LowerBox">
            <span style="float:left;">
                <img id="imgCadworksLogo" src="@Url.Content("~/Content/Images/ByCADworksLogo.png")"></span>
                <span style="float:right;"><input type="image" id="btnHelp" src="@Url.Content("~/Content/Images/HelpButton.png")"></span>
            </div>
        </header>
        <section>
            @RenderBody()
        </section>
        <footer>
        <div id="Footer2" style="color: white; width:100; border:1px solid white;">
           <span style="width:inherit;">
                <span style="float:left; font-size:small;">©@DateTime.Now.Year ENGworks. All Rights Reserved.</span>
                <span style="float:right; font-size:small;"><a id="Eula" href="#" style="color:White;text-decoration:none;">EULA</a> | <a id="Help" href="#" style="color:White;text-decoration:none;">Help</a>
                </span>
                </span>
         </div>
        </footer>
    </div>
</body>
</html>
